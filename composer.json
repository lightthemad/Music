  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "secure-http": false
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "project-install": "./scripts/project-install.sh",
    "project-install:prod": "./scripts/project-install.sh prod"
  },
  "extra": {
    "patches": {
      "drupal/addanother": {
        "Fix broken add another URL": "https://www.drupal.org/files/issues/addanother-url-doesnt-match-machine-name-2851584-3.patch"
      },
      "drupal/context": {
        "Fix 'Cannot use scalar value as array' error": "https://www.drupal.org/files/issues/use_scalar_value_as_array-2865345-5.patch"
      },
      "drupal/statistics_counter": {
        "Save last cron run time in state instead of cron": "https://www.drupal.org/files/issues/use_state_instead_of-2693019-2.patch"
      },
      "drupal/migrate_tools": {
        "WSOD: You have requested a non-existent service": "https://www.drupal.org/files/issues/Migrate_tools_2_0_beta_1_d8_core_3.5-2878169-10.patch"
      },
      "drupal/image_formatter_link_to_image_style": {
        "Image style URL is not generated with token": "https://www.drupal.org/files/issues/image_formatter_link_to_image_style-access_denied-2739037-4.patch"
      },
      "drupal/block_node": {
        "Add route context to block": "patches/block_node_cache_contexts.patch"
      },
      "drupal/core": {
	    "Access user emails " : "https://www.drupal.org/files/issues/2019-04-26/drupal-2799049-30-new-permission-view-user-mail.patch",
        "Fix Entity reference autocomplete with fields": "https://www.drupal.org/files/issues/2019-04-16/2174633-views-273.patch",
        "Fix ajax error in views relationships" : "https://www.drupal.org/files/issues/2019-05-04/2457999-194.patch"
      },
      "drupal/video_embed_facebook": {
        "iframe fix": "https://www.drupal.org/files/issues/2019-05-13/facebook_embed_field.patch"
      },
      "drupal/page_manager": {
        "Fix title change bug": "https://www.drupal.org/files/issues/2018-04-17/page_manager-page-label-not-editable-2830464-10.patch"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "excludes": [
        ".editorconfig"
      ]
    }
  }
}
